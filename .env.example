# Example Environment Variables
# Copy this file to .env and fill in your values
# The .env file is gitignored and will not be committed

# GCP Project Configuration
GCP_PROJECT_ID=YOUR_PROJECT_ID
GCP_REGION=us-central1

# GCS Buckets
GCS_JSONL_BUCKET=YOUR_PROJECT_ID-data-discovery-jsonl
GCS_REPORTS_BUCKET=YOUR_PROJECT_ID-data-discovery-reports

# Vertex AI Search
VERTEX_DATASTORE_ID=data-discovery-metadata
VERTEX_LOCATION=global

# Optional: Service Account Key (if not using default credentials)
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json
